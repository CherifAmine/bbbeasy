{
  "name": "hivelvet-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^4.19.5",
    "axios": "^0.26.1",
    "craco-less": "^2.0.0",
    "dotenv-cra": "^3.0.2",
    "eslint-plugin-react-hooks": "^4.4.0",
    "i18next": "^21.6.13",
    "moment": "^2.29.2",
    "perfect-scrollbar": "^1.5.5",
    "pg": "^8.7.3",
    "pino": "^7.6.4",
    "pino-pretty": "^7.6.1",
    "pino-transmit-http": "^1.0.1",
    "pm2": "^5.1.2",
    "rc-color-picker": "^1.2.6",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-highlight-words": "^0.18.0",
    "react-hook-form": "^7.29.0",
    "react-i18next": "^11.15.5",
    "react-intl": "^5.24.8",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.1",
    "reload": "^3.2.0",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0",
    "websocket": "^1.0.34"
  },
  "scripts": {
    "start-dev": "./hivelvet.sh -ew && PORT=3300 NODE_ENV=development INSTALLER_FEATURE=false craco start",
    "start-dev-installer": "./hivelvet.sh -ei && PORT=3300 NODE_ENV=development INSTALLER_FEATURE=true craco start",
    "build": "./hivelvet.sh -ew && INSTALLER_FEATURE=false craco build",
    "build-installer": "./hivelvet.sh -ei && INSTALLER_FEATURE=true craco build && ./hivelvet.sh -ew",
    "nyc": "nyc instrument --compact=false src instrumented --lines 0",
    "nyc-summary": "nyc report --reporter=text-summary --lines 0",
    "cypress-dev": "./hivelvet.sh -ew && cypress run --config '{\"excludeSpecPattern\":[\"**/installer/*/*.js\",\"**/installer/*.js\"]}' && npm run nyc-summary --lines 0",
    "cypress-dev-installer": "./hivelvet.sh -ei && cypress run --config '{\"excludeSpecPattern\":[\"**/webapp/*/*.js\",\"**/webapp/*.js\"]}' && ./hivelvet.sh -ew && npm run nyc-summary --lines 0",
    "analyze": "craco analyze",
    "test": "craco test",
    "eject": "craco eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'",
    "isready": "npm run format && npm run lint && npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11"
    ],
    "development": [
      "defaults",
      "not IE 11"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@craco/craco": "^6.4.3",
    "@crowdin/cli": "3",
    "@cypress/code-coverage": "^3.9.12",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^14.0.1",
    "@types/node": "^17.0.23",
    "@types/pino": "^7.0.5",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/parser": "^5.18.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cypress": "^10.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-webpack-plugin": "^3.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "react-styleguidist": "^11.2.0",
    "simple-progress-webpack-plugin": "^2.0.0",
    "source-map-explorer": "^2.5.2",
    "ts-loader": "^9.2.6"
  }
}
